{
  "schemaVersion": 39,
  "title": "Slow Service",
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-30m", "to": "now" },
  "panels": [
    {
      "type": "timeseries",
      "title": "Average duration",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "rate(service_duration_seconds_sum[5m]) / rate(service_duration_seconds_count[5m])",
          "legendFormat": "avg"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "p95 duration",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(service_duration_seconds_bucket[5m])))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
    },
    {
      "type": "timeseries",
      "title": "By operation",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "rate(service_duration_seconds_sum[5m]) / rate(service_duration_seconds_count[5m])",
          "legendFormat": "{{operation}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 }
    }
  ],
  "templating": { "list": [] }
}
